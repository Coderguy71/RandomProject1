{
  "name": "sat-math-learning-platform",
  "version": "1.0.0",
  "description": "SAT Math Learning Platform - Full Stack Application",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "dev:full": "concurrently \"npm run dev\" \"cd frontend && npm run dev\"",
    "db:migrate": "db-migrate up",
    "db:migrate:down": "db-migrate down",
    "db:migrate:create": "db-migrate create",
    "frontend:install": "cd frontend && npm install",
    "frontend:build": "cd frontend && npm run build",
    "frontend:dev": "cd frontend && npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "sat",
    "learning",
    "platform",
    "express",
    "postgresql",
    "react",
    "vite",
    "tailwind"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^6.10.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2"
  }
}